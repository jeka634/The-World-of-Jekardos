    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.8.3",
    "sqlite3": "^5.1.7",
    "uuid": "^13.0.0",
    "@vitejs/plugin-react": "^5.0.0",
    "vite": "^6.2.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
